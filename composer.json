{
  "name": "ergebnis/rector-config-template",
  "description": "Provides a configuration factory and multiple rule sets for rector/rector.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/rector-config-template",
  "support": {
    "issues": "https://github.com/ergebnis/rector-config-template/issues",
    "source": "https://github.com/ergebnis/rector-config-template",
    "security": "https://github.com/ergebnis/rector-config-template/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
    "rector/rector": "~0.18.11"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.40.0",
    "ergebnis/data-provider": "^3.2.0",
    "ergebnis/license": "^2.4.0",
    "ergebnis/php-cs-fixer-config": "^6.13.1",
    "ergebnis/phpunit-slow-test-detector": "^2.7.0",
    "fakerphp/faker": "^1.22.0",
    "infection/infection": "~0.27.9",
    "phpunit/phpunit": "^10.5.2",
    "psalm/plugin-phpunit": "~0.18.4",
    "vimeo/psalm": "^5.17.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Rector\\Config\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Rector\\Config\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
